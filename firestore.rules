rules_version = '2';
service cloud.firestore {
  match /databases/{database}/documents {
    match /quotes/{document} {
      allow create: if true;
      allow read, update, delete: if false;
    }
    match /contacts/{document} {
      allow create: if true;
      allow read, update, delete: if false;
    }
    match /posts/{document} {
      allow read: if true;
      allow write: if false;
    }
  }
}
